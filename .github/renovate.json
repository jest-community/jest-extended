{
  "extends": ["config:recommended", ":semanticCommitsDisabled"],
  "lockFileMaintenance": {
    "enabled": true,
    "automerge": true
  },
  "rangeStrategy": "replace",
  "postUpdateOptions": ["yarnDedupeHighest"],
  "packageRules": [
    {
      "matchPackageNames": [
        "@codesandbox/sandpack-react",
        "@codesandbox/sandpack-themes"
      ],
      "groupName": "sandpack"
    }
  ]
}
